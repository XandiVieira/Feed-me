<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavigationViewTextStyle">
        <item name="android:fontFamily">@font/quicksand_bold</item>
    </style>

    <style name="customRoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="color">@color/white</item>
        <item name="marginHorizontal">8dp</item>
        <item name="cornerSizeBottomLeft">96dp</item>
    </style>

    <style name="customRoundedImageView2" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">0dp</item>
        <item name="color">@color/white</item>
        <item name="marginHorizontal">8dp</item>
        <item name="cornerSizeBottomRight">96dp</item>
    </style>

    <style name="custom_rating_bar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_rating_bar</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
        <item name="android:maxWidth">16dp</item>
        <item name="android:minWidth">16dp</item>
    </style>

</resources>